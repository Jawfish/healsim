[application]

config/name="healsim"
run/main_scene="res://Interface/MainMenu.tscn"

[autoload]

SceneManager="*res://Engine/Managers/SceneManager.tscn"
SignalManager="*res://Engine/Managers/SignalManager.gd"
InputManager="*res://Engine/Managers/InputManager.gd"
DebugManager="*res://Engine/Managers/DebugManager.tscn"
